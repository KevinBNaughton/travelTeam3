<header>
<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">


  <nav class="navbar navbar-expand-lg navbar-light bg-light">
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarTogglerDemo01" aria-controls="navbarTogglerDemo01" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarTogglerDemo01">
      <a><%= link_to image_tag("final.png", alt: "Company logo"), root_path %></a>
      <ul class="navbar-nav mr-auto mt-2 mt-lg-0" id="box-wrap">
        <li class="nav-item" id="box">
          <%= link_to "Home", root_path, id: "logo" %>
        </li>
        <li class="nav-item" id="box">
          <%= link_to "About Us", contact_path %>
        </li>
        <li class="nav-item" id="box">
          <%= link_to "Admin", admin_path %>
        </li>
      </ul>

      <% if logged_in? %>
        <a class="form-inline my-2 my-lg-0" style="width: auto">
          <%= link_to "Log out", logout_path, method: :delete %>
        </a>
      <% else %>
        <a class="form-inline my-2 my-lg-0" style="width: auto">
          <%= link_to "Sign up", signup_path %>
        </a>
        <a class="form-inline my-2 my-lg-0" style="width: auto">
          <%= link_to "Log in", login_path %>
        </a>
      <% end %>


    </div>
  </nav>
</header>
